{
  "name": "eslint-config-redtn",
  "version": "0.1.2",
  "description": "Basic ESLint config",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RedTn/eslint-config-redtn"
  },
  "license": "ISC",
  "author": "Redmond Tran",
  "main": "index.js",
  "scripts": {
    "depcheck": "depcheck",
    "format": "prettier --write \"*.js\" \"{**,.}/*.js\"",
    "format:check": "prettier --check \"*.js\" \"{**,.}/*.js\"",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "sort-package-json": "sort-package-json"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "depcheck": "^0.9.2",
    "eslint": "~7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "sort-package-json": "^1.44.0"
  },
  "peerDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "~7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4"
  }
}
