{
  "name": "eslint-config-redtn-react",
  "version": "0.2.1",
  "description": "ESLint config for react projects",
  "keywords": [
    "eslint",
    "eslint-config",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RedTn/eslint-config-redtn"
  },
  "license": "ISC",
  "author": "Redmond Tran",
  "main": "index.js",
  "scripts": {
    "depcheck": "depcheck",
    "format": "prettier --write \"*.js\" \"{**,.}/*.js\"",
    "format:check": "prettier --check \"*.js\" \"{**,.}/*.js\"",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "sort-package-json": "sort-package-json"
  },
  "dependencies": {
    "eslint-config-redtn": "^0.1.3"
  },
  "devDependencies": {
    "depcheck": "^1.4.1",
    "eslint": "~7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "prettier": "^2.0.5"
  },
  "peerDependencies": {
    "eslint": "~7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4"
  }
}
